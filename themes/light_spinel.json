      "settings": {
        "foreground": "#d431f5"
      }
    },
    {
      "name": "Method and function calls",
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#323333"
      }
    },
    {
      "scope": "meta.attribute.rust",
      "settings": {
        "foreground": "#ce74f8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "diff: header",
      "scope": [
