    {
      "name": "diff: deleted",
      "scope": ["markup.deleted", "punctuation.definition.deleted.diff"],
      "settings": {
        "fontStyle": "",
        "foreground": "#d12f2f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "fontStyle": "",
        "foreground": "#ed8907"
      }
    },
    {
      "name": "diff: inserted",
      "scope": ["markup.inserted", "punctuation.definition.inserted.diff"],
      "settings": {
        "foreground": "#0e9925"
      }
    }
