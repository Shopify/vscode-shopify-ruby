      "settings": {
        "foreground": "#0e9925"
      }
    },
    {
      "scope": "meta.embedded.line",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Regexp",
      "scope": ["string.regexp", "punctuation.section.regexp"],
      "settings": {
        "foreground": "#79b002",
        "fontStyle": ""
